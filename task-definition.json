{
    "family": "nginx",
    "taskRoleArn": "arn:aws:iam::719378226820:role/ECSTaskRole",
    "executionRoleArn": "arn:aws:iam::719378226820:role/ECSTaskRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "nginx",
            "image": "nginx",
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}